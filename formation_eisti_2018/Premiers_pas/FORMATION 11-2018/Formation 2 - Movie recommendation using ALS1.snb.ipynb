    {
      "metadata" : {
        "trusted" : true,
        "input_collapsed" : false,
        "collapsed" : false,
        "id" : "0536D7435A3D47C0880AF723B328E582"
      },
      "cell_type" : "code",
      "source" : [
        "val splits = ratingsDF.randomSplit(Array(0.75, 0.25), seed = 12345L)\n",
        "val (trainingData, testData) = (splits(0), splits(1))\n",
        "\n",
        "val numTraining = trainingData.count()\n",
        "val numTest = testData.count()\n",
        "\n",
        "println(\"Training: \" + numTraining + \" test: \" + numTest)"
      ],
      "outputs" : [
        {
